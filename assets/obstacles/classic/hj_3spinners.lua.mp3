function init()
	offset = math.random(0, 100)

	spinner0 = mgCreateBody()
	mgMaterial("glass")
	mgCreateBox(spinner0, 3, .5, 0.05, 0, .2, 0)

	spinner1 = mgCreateBody()
	mgMaterial("glass")
	mgCreateBox(spinner1, 3, .5, 0.05, 0, 1.3, 0)

	spinner2 = mgCreateBody()
	mgMaterial("glass")
	mgCreateBox(spinner2, 3, .5, 0.05, 0, 2.4, 0)

	spinnerpipe = mgCreateBody()
	mgMaterial("steel")
	mgStatic(1)
	mgMask(0)
	mgCreateCylinder(spinnerpipe, 0.075, 3, 2, 2, 0, 0, 0, 1.57)
	mgCreateCylinder(spinnerpipe, 0.075, 3, -2, 2, 0, 0, 0, 1.57)

end

function tick()

	mgMove(spinner0, 2, 0, 0, 10)
	mgRotate(spinner0, 0, mgTime(), 0, 10, 1)

	mgMove(spinner1, -2, 0, 0, 10)
	mgRotate(spinner1, 0, -mgTime(), 0, 10, 1)

	mgMove(spinner2, 2, 0, 0, 10)
	mgRotate(spinner2, 0, mgTime(), 0, 10, 1)
	
end

