	mgMaterial("steel")
	mgStatic(1)
	mgCreateBox(b, 0.7, 0.05, 0.7, 0, -0.5, 0)

	mgMaterial("glass")
	mgColor(0.7, 1, 0.7)
	mgCreateBox(b, 0.5, 0.01, 0.5, 0, 1.0, 0)
	mgCreateBox(b, 0.01, 1.0, 0.5, -0.5, 0, 0)
	mgCreateBox(b, 0.01, 1.0, 0.5, 0.5, 0, 0)
	mgCreateBox(b, 0.5, 1.0, 0.01, 0, 0, 0.5)
	mgCreateBox(b, 0.5, 1.0, 0.01, 0, 0, -0.5)
	
	mgPowerUp("diamond", 0, 1, 0)
end


