function init()
	offset = math.random(0, 100)

	spinner0 = mgCreateBody()
	mgMaterial("glass")
	mgCreateBox(spinner0, 3, .5, 0.05, 0, 1, 0)

	spinnerpipe = mgCreateBody()
	mgMaterial("steel")
	mgStatic(1)
	mgMask(0)
	mgCreateCylinder(spinnerpipe, 0.075, 3, 0, 2, 0, 0, 0, 1.57)

end

function tick()

	mgMove(spinner0, 0, 0, 0, 10)
	mgRotate(spinner0, 0, -mgTime(), 0, 10, 1)
	
end

